		System.out.println(x % 5);
		
		*/
		//compound operators
		int x = 10;
		
		x = x + 8;// x+= 8
		System.out.println(x);
		x = x - 4;// x-= 4
		System.out.println(x);
		x = x * 12;// x *= 12
		System.out.println(x);
		x = x / 6;// x /= 6
		System.out.println(x);
		x = x % 3;// x %= 3
		System.out.println(x);
		
		x++;
		System.out.println(x);
		x--;
		System.out.println(x);
		
	}

}
