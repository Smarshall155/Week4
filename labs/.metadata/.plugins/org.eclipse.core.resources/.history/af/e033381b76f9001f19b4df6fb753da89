
public class HelloWorld {

	public static void main(String[] args) {

		System.out.println("hello World. ");
		/*
		//primitive variables
		//numerical
		byte a; //declaration 
		a =35; //initialization
		byte b = 65;
		short c = 3232;
		int d = 233234;// integer used most often
		long f = 93890245;
		
		//precision variable
		float g = 45.65f;
		double h = 8.87343; //this is the one you most commonly use.
		
		//boolean
		boolean t = true;// or false
		
		char x = 'A';
		System.out.println(x);
		
		//advanced
		String name = "Stephen Marshall";
		
		int y = 2323;
		//concatenation
		System.out.println("y is equal to " + y + "!");
		*/
		
		//logical operators
		int x = 10;
		//addition -> +
		//subtraction -> -
		//multiplication -> *
		//division -> /
		//modulus -> %
		int x = 10;
		
		System.out.println(x + 5);
		System.out.println(x - 5);
		System.out.println(x * 5);
		System.out.println(x / 5);
		System.out.println(x % 5);
	}

}
